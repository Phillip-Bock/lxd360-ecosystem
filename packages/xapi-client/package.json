{
  "name": "@inspire/xapi-client",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.js"
    },
    "./builder": {
      "types": "./dist/builder/index.d.ts",
      "import": "./dist/builder/index.js",
      "require": "./dist/builder/index.js"
    },
    "./recipes": {
      "types": "./dist/recipes/index.d.ts",
      "import": "./dist/recipes/index.js",
      "require": "./dist/recipes/index.js"
    },
    "./validator": {
      "types": "./dist/validator/index.d.ts",
      "import": "./dist/validator/index.js",
      "require": "./dist/validator/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@inspire/types": "*",
    "uuid": "^10.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.3"
  }
}
